{

  "manifest_version": 2,
  "name": "LBS to KG",
  "version": "1",

  "description": "Automatically converts weight written in pounds to kg.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["background.js"]
    }
  ]

}